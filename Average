class Average {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        int i=0;
        int sum=0;
        while(i<n){
            sum+=arr[i];
            i++;
        }
        int avg=sum/n;
        System.out.println("Average :"+avg);
    }
}
